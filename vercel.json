{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/character.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/character-recommend.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/character",
      "dest": "/api/character.js"
    },
    {
      "src": "/api/character-recommend",
      "dest": "/api/character-recommend.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "GROQ_API_KEY": "your-groq-api-key",
    "GROQ_MODEL": "llama-3.1-8b-instant"
  }
}