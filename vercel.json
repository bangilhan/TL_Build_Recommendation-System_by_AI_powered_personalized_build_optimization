{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/character.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/character-recommend.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/character",
      "dest": "/api/character.js"
    },
    {
      "src": "/api/character-recommend",
      "dest": "/api/character-recommend.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "LLM_API_BASE": "http://172.20.92.48:30410/v1",
    "LLM_API_KEY": "sk-local",
    "LLM_MODEL": "Qwen2.5-72B"
  }
}